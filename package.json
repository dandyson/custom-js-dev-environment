{
  "name": "modern-js-dev-environment",
  "type": "module",
  "version": "1.0.0",
  "description": "My version of a js dev env from Cory House's Pluralsight course 'Building a Javascript Development Environment'",
  "main": "./src/index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "localtunnel": "lt --port 3000 --subdomain js-dev-env",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.3.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^7.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-watch": "^8.0.0",
    "express": "^4.19.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.3.0",
    "webpack-dev-server": "^5.0.4",
    "webpack-hot-middleware": "^2.26.1"
  },
  "dependencies": {
    "numeral": "^2.0.6",
    "openurl": "^1.1.1"
  }
}
